  "bugs": {
    "url": "https://github.com/LeeSimonH/Solo-Project/issues"
  },
  "homepage": "https://github.com/LeeSimonH/Solo-Project#readme",
  "dependencies": {
    "concurrently": "^7.1.0",
    "ejs": "^3.0.2",
    "express": "^4.12.3",
    "mongoose": "^6.3.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.17.9",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "babel-loader": "^8.2.5",
    "cross-env": "^7.0.3",

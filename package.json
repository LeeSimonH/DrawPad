{
  "name": "solo-project",
  "version": "1.0.0",
  "description": "CodeSmith Solo Project - started Tuesday, April 19, 2022",
  "main": "index.js",
  "scripts": {
    "start": "nodemon server/server.js",
    "build": "export NODE_ENV=production; webpack",
    "dev-server": "export NODE_ENV=development; webpack serve",
    "dev": "concurrently \"cross-env NODE_ENV=production nodemon server/server.js\" \"cross-env NODE_ENV=development webpack serve --open\""
  },
  "nodemonConfig": {
    "ignore": [
      "build",
      "client"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LeeSimonH/Solo-Project.git"
  },
  "keywords": [],
